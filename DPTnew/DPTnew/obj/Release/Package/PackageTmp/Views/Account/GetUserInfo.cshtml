@model DPTnew.Models.Contact

@if (Model != null)
{
    @DPTnew.Localization.Resource.View_HomeIndex_Hello
    <b>
        @Model.FirstName.ToString() @Model.LastName.ToString()
    </b>
    <p>
        @DPTnew.Localization.Resource.View_HomeIndex_of @ViewBag.Company
    </p>
    <p>
        @DPTnew.Localization.Resource.AccountNumber <b>@ViewBag.AccNumber </b>
    </p>
}


